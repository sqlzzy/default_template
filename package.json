{
    "name": "default_template",
    "version": "1.0.0",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/osipovsergey/default_template.git"
    },
    "author": "Sergey Osipov",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.12.3",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/preset-env": "^7.12.1",
        "@vue/test-utils": "^1.1.0",
        "babel-core": "^6.26.3",
        "babel-jest": "^26.6.1",
        "babel-loader": "^8.1.0",
        "babel-preset-env": "^1.7.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^6.2.1",
        "cross-env": "^7.0.2",
        "css-loader": "^5.0.0",
        "css-minimizer-webpack-plugin": "^1.1.5",
        "html-loader": "^1.3.2",
        "html-webpack-plugin": "^5.0.0-alpha.7",
        "jest": "^26.6.1",
        "less": "^3.12.2",
        "less-loader": "^7.0.2",
        "mini-css-extract-plugin": "^1.2.1",
        "optimize-css-assets-webpack-plugin": "^5.0.4",
        "postcss-loader": "^4.0.4",
        "postcss-preset-env": "^6.7.0",
        "style-loader": "^2.0.0",
        "terser-webpack-plugin": "^5.0.2",
        "vue-jest": "^3.0.7",
        "vue-loader": "^15.9.4",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.6.12",
        "webpack": "^5.3.0",
        "webpack-bundle-analyzer": "^3.9.0",
        "webpack-cli": "^4.1.0",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^5.2.0"
    },
    "description": "Webpack + Vue + Less",
    "bugs": {
        "url": "https://github.com/osipovsergey/default_template/issues"
    },
    "homepage": "https://github.com/osipovsergey/default_template#readme",
    "scripts": {
        "test": "jest",
        "dev": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
        "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
        "genstats": "webpack --profile --json > ./stats/stats.json",
        "watchstats": "webpack-bundle-analyzer ./stats/stats.json ./dist"
    },
    "dependencies": {
        "vue": "^2.6.12",
        "vue-template-loader": "^1.1.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "vue"
        ],
        "transform": {
            ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
            "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
        },
        "collectCoverage": true,
        "collectCoverageFrom": [
            "**/*.{js,vue}",
            "!**/node_modules/**"
        ],
        "coverageReporters": [
            "html",
            "text-summary"
        ]
    }
}